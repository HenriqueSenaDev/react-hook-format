{
	"name": "react-hook-format",
	"version": "1.1.7",
	"description": "Simplify number and field formatting in JavaScript with React Hook Format. A lightweight and flexible library that provides an uncomplicated way to format and present data consistently. Effortlessly transform your formatting operations, making data manipulation a swift and efficient task.",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"lib:publish": "npm publish --no-git-checks",
		"lib:build": "rollup -c",
		"lib:lint": "eslint ./**/*.ts",
		"lib:lint-fix": "eslint ./**/*.ts --fix",
		"lib:format": "prettier --write \"**/*.{js,jsx,json,md}\""
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/node": "^20.11.0",
		"@types/react": "^18.2.47",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"prettier": "^3.2.2",
		"react": "^18.2.0",
		"rollup": "^4.9.5",
		"rollup-plugin-dts": "^6.1.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3"
